{
    "name": "unscrew/unscrew",
    "description": "Markdown-to-JSON headless CMS",
    "type": "library",
    "require": {
        "php": "~8.1",
        "symfony/mime": "6.4",
        "league/commonmark": "^2.4",
        "thecodingmachine/safe": "^2.5",
        "symfony/http-foundation": "6.4.14",
        "symfony/string": "^6.4|^7.0",
        "symfony/translation-contracts": "^3.5",
        "league/flysystem": "^3.0",
        "symfony/yaml": "6.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Unscrew\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "adrian7",
            "email": "adrian.silimon@yahoo.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "pestphp/pest": "^3.5",
        "league/flysystem-memory": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
